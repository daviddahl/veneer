{
  "name": "veneer",
  "version": "0.0.1",
    "main": "dist/index.js",  
    "scripts": {
      "build": "npm run clean && npm run build:dev && npm run build:prd",
      "build:dev": "webpack src/index.js dist/index.js --config tools/webpack.config.dev.js",
      "build:prd": "webpack src/index.js dist/index.min.js --config tools/webpack.config.prd.js",
      "prepublish": "npm run build",
      "clean": "rimraf dist build",
      "lint": "eslint src",
      "test": "karma start --single-run --no-auto-watch karma.config.js",
      "build-orig": "webpack --progress --colors && cp ./test/index.html ./dist/test/index.html",
      "build-test": "webpack --progress --colors --config=webpack.test.config.js && cp ./test/index.html ./dist/test/index.html"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-eslint": "^4.1.6",
    "babel-istanbul": "^0.6.0",
    "babel-istanbul-loader": "0.0.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "webpack": "^1.12.13",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "jasmine-core": "^2.4.1",
    "karma": "https://github.com/karma-runner/karma.git#master",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "*",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "loader-utils": "^0.2.12",
    "rimraf": "^2.5.0"
  },
  "dependencies": {
    "browserify": "^13.0.0"
  },
    "author": "David Dahl (https://nulltxt.se)",
    "license": "MIT"
}

