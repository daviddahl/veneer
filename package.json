{
  "name": "veneer",
  "version": "0.0.1",
  "scripts": {
    "buildorig": "./node_modules/.bin/babel ./src --out-file ./dist/veneer.js --source-maps inline && ./node_modules/.bin/babel ./test --out-file ./dist/test/veneer-tests.js --source-maps inline && cp ./test/index.html ./dist/test/index.html",
    "build": "webpack --progress --colors && cp ./test/index.html ./dist/test/index.html",
    "build-test": "webpack --progress --colors --config=webpack.test.config.js && cp ./test/index.html ./dist/test/index.html"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "webpack": "^1.12.13",
    "babel-preset-stage-0": "^6.3.13"
  },
  "dependencies": {
    "browserify": "^13.0.0"
  }
}

